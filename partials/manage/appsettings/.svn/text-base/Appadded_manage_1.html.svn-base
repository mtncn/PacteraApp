<div class="conRight_main">
    <form name="upApp1" novalidate>
    <div class="pop_form">
        <h4 class="noback">
            <span>发布新应用</span>
        </h4>
        <div class="step_til">
            <p>
                <a href="###">基本信息</a>
                <a href="###">应用详情</a>
                <a href="###">发布应用</a>
                <a href="###">发布范围</a>
                <a href="###" class="over">发布完成</a>
            </p>
        </div>
        <div class="step_til_p1">
            <p class="step_til_p1L">
                <span>应用名称</span><br>
                <input ng-model="putup.AppName" type="text" name="AppName" required/>
                <span class="help-block" ng-messages="upApp1.AppName.$error" ng-if="upApp1.AppName.$touched">
                    <span ng-message="required">应用名称必填</span>
                </span>
            </p>
            <p>
                <span>版本号</span><br>
                <input ng-model="putup.Version" type="text" name="Version" required/>
                <span class="help-block" ng-messages="upApp1.Version.$error" ng-if="upApp1.Version.$touched">
                    <span ng-message="required">版本号必填</span>
                </span>
            </p>
        </div>
        <div class="step_til_p1">
            <p class="step_til_p1L">
                <span>语言</span><br>
                <select class="inp-WIDTH" ng-model="putup.Language"  ng-options="x.key as x.language for x in languages"/>
            </p>
            <p>
                <span>类别</span><br>
                <select class="inp-WIDTH" ng-model="putup.Type"  ng-options="x.key as x.type for x in types"/>
            </p>
        </div>

        <div class="step_til_p1">
            <span>描述</span><br>
            <textarea ng-model="putup.Describe" placeholder="应用描述，40字以内"></textarea>
        </div><br>
        <p class="his_dat_last his_dat_last_no1">

            <button type="submit" ng-click="Save_No1(putup)"
                    ng-disabled="upApp1.AppName.$invalid || upApp1.Version.$invalid">
                保存
            </button>
            <button type="submit" class="his_dat_last_btn" ng-click="Next(putup)" ui-sref="manage.appadded2"
                    ng-disabled="upApp1.AppName.$invalid || upApp1.Version.$invalid">
                下一步
            </button>
        </p>
    </div>
    </form>
</div>