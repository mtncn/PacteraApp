.application_state_manbusapp{
    width: 90%;
    height: 110px;
    margin-left: 20px;
    border: 1px solid #ccc;
    margin-top: 25px;
}
.application_state_manbusapp h4{
    width: 100%;
    height: 35px;
    line-height: 35px;
    font-size: 12px;
    background: #f5f5f5;
}
.application_state_manbusapp h4 span{
    margin-left: 20px;
    color: #999;
}
.application_state_manbusapp h4 b{
    margin-left: 30px;
}
.application_left_manbusapp{
    width: 35%;
    float: left;
}
.application_manbusapp{
    margin-top: 20px;
}
.application_left_manbusapp dt{
    float: left;
    width: 20%;
    margin-left: 15px;
}
.application_left_manbusapp dt img{
    width: 50px;
    height: 50px;
    border-radius: 5px;
}
.application_left_manbusapp dd{
    width: 70%;
    font-size: 12px;
    float: right;
}
.application_left_manbusapp dd .p1{
    margin-top: 5px;
}

.application_right_manbusapp{
    font-size: 12px;
    width: 60%;
    float: left;
    margin-left: 35px;
}
.application_right_manbusapp .span1_manbusapp{
    color: #7a7a7a;
}
.application_right_manbusapp button{
    padding: 3px 15px;
    border-radius: 5px;
    background: #334961;
    color: #fff;
    margin: 0 15px;
    font-size: 12px;
    width: 20%;
}

/*Created 2016.03.24 End*/


